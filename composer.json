{
    "name": "gerenuk/filament-banhammer",
    "description": "This package adds model banning functionality to filament",
    "keywords": [
        "Gerenuk",
        "laravel",
        "filament",
        "banhammer",
        "filament-banhammer"
    ],
    "homepage": "https://github.com/gerenuk-ltd/filament-banhammer",
    "license": "MIT",
    "authors": [
        {
            "name": "Kieran Proctor",
            "email": "kieran.lee.proctor@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2|^4.0",
        "mchev/banhammer": "^2.4.2",
        "spatie/laravel-package-tools": "^1.92.7"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^3.4",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34|^3.0",
        "pestphp/pest-plugin-arch": "^2.7|^3.0",
        "pestphp/pest-plugin-laravel": "^2.3|^3.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^1.1|2.0",
        "phpstan/phpstan-phpunit": "^1.3|^2.0",
        "spatie/laravel-ray": "^1.35|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Gerenuk\\FilamentBanhammer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gerenuk\\FilamentBanhammer\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-filament-banhammer --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gerenuk\\FilamentBanhammer\\FilamentBanhammerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
